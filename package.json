{
  "name": "aws-cdk-template",
  "version": "0.1.0",
  "private": "true",
  "bin": {
    "aws-cdk-template": "bin/entrypoint.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:update": "jest -u",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy '*'",
    "deploy:never": "cdk deploy '*' --require-approval never",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "aws-cdk-lib": "2.203.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^30.0.0",
    "@types/node": "22.16.0",
    "aws-cdk": "2.1020.1",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=22.17.0"
  }
}
